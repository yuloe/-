.chooseRank {
  display: flex;
  justify-content: space-around;

  width: 650rpx;
  margin-right: auto;
  margin-left: auto;
  margin-top: 6vh;
  margin-bottom: 4vh;
}

.chooseRank .rank {
  display: flex;
  justify-content: center;
  align-items: center;

  width: 180rpx;
  height: 120rpx;
  border-radius: 20rpx;

  font-size: 20px;
}

.active1 {
  border: 6px solid #fa8072;
  font-size: 25px;
  font-weight: bold;
}

.active2 {
  border: 6px solid #00bfff;
  font-size: 25px;
  font-weight: bold;
}

.active3 {
  border: 6px solid #8470ff;
  font-size: 25px;
  font-weight: bold;
}

.startTest {
  display: flex;

  width: 500rpx;
  height: 10vh;
  border: 1px solid #d88a63;
  border-radius: 20rpx;
  background-color: #f5b619;
  font-size: 30px;

  justify-content: center;
  align-items: center;
}

.highest {
  display: flex;
  justify-content: center;
  align-items: center;

  height: 150rpx;
  font-size: 16px;
}

.testHistory {
  display: flex;
  align-items: center;
  flex-direction: column;

  width: 600rpx;
  height: 270rpx;
  border: 1px solid #f5b619;
  border-radius: 20rpx;
  background-color: #ffdead;
}

.title {
  display: flex;
  justify-content: space-around;
  font-size: 30rpx;

  width: 100%;

  white-space: pre;
  text-align: center;
}

.record {
  width: 20%;
}

.time {
  width: 30%;
}

.score {
  width: 20%;
}

.testHistoryRecord view {
  display: flex;
  justify-content: space-around;
  font-size: 30rpx;

  white-space: pre;
}